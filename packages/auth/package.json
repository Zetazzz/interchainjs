{
  "name": "@interchainjs/auth",
  "version": "0.0.6",
  "description": "Authentication for web3 accounts",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "author": "Cosmology <developers@cosmology.zone>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE-MIT ../../LICENSE-Apache README.md package.json dist",
    "clean": "del dist/**",
    "build": "npm run clean; tsc; tsc -p tsconfig.esm.json",
    "pub": "npm version patch && npm run copy && cd dist && npm publish --access public"
  },
  "dependencies": {
    "@interchainjs/types": "^0.0.9",
    "@interchainjs/utils": "^0.0.8",
    "@noble/curves": "^1.1.0",
    "@noble/hashes": "^1.3.1"
  },
  "keywords": [
    "cryptography",
    "secp256k1",
    "ed25519",
    "wallet"
  ]
}
