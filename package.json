{
  "name": "sign",
  "version": "0.0.1",
  "private": true,
  "description": "sign client for block-chain networks",
  "repository": "git@github.com:cosmology-tech/sign.git",
  "author": "liujun93 <ljun8575@gmail.com>",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "networks/*"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "symlink": "yarn link @uni-sign/auth @uni-sign/types @uni-sign/utils @uni-sign/cosmos-msgs @uni-sign/cosmos-query @uni-sign/cosmos @uni-sign/injective @uni-sign/ethereum",
    "starship:start": "cd __test__ && ./scripts/start-server.sh",
    "starship:stop": "cd __test__ && ./scripts/stop-server.sh",
    "starship:restart": "cd __test__ && ./scripts/stop-server.sh && ./scripts/start-server.sh",
    "link": "lerna exec -- yarn link",
    "clean": "lerna run clean",
    "pub": "lerna run pub"
  },
  "devDependencies": {
    "@cosmjs/amino": "^0.32.2",
    "@cosmjs/proto-signing": "^0.32.3",
    "@cosmjs/stargate": "^0.32.2",
    "@cosmology/telescope": "^1.4.11",
    "@injectivelabs/sdk-ts": "^1.14.5",
    "@injectivelabs/wallet-ts": "^1.14.7",
    "@noble/hashes": "^1.4.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "ethers": "^5.7.2",
    "jest": "^29.6.2",
    "lerna": "^7.4.2",
    "prettier": "^3.0.2",
    "strip-ansi": "^6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@1.22.1"
}